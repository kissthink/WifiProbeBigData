.datatime_input_style{
    margin-top:13px;
}

.float_right{
    float:right;
}

 .check_input{
  
  font-weight: 300;
  font-size: 16px;
  text-decoration: none;
  text-align: center;
  line-height: 40px;
  height: 30px;
  padding: 0 25px;
  margin: 0;
  display: inline-block;
  appearance: none;
  cursor: pointer;
  border: none;
  background-color:#D0D0D0;
}

.add-on{
   background:white;
}

.background_color{
    background-color:#F0F0F0;
}

.datashow_background{
   clear:both;
   border:1px solid #ADADAD;
   margin-top:150px;
   box-shadow:3px 3px 20px #ADADAD;
   margin-left:auto;
   margin-right:auto;
}

 .boot{
  clear:both;
  font-weight: 300;
  font-size: 16px;
  text-decoration: none;
  text-align: center;
  line-height: 40px;
  height: 10%;
  padding: 0 3%;
  margin: 0;
  display: inline-block;
  appearance: none;
  cursor: pointer;
  background-color:#009900;
  position:relative;
  top:10px;
  left:30%;
  margin-left:auto;
  margin-right:auto;
}

 .shutdown{
  clear:both;
  font-weight: 300;
  font-size: 16px;
  text-decoration: none;
  text-align: center;
  line-height: 40px;
  height: 10%;
  padding: 0 3%;
  margin: 0;
  display: inline-block;
  appearance: none;
  cursor: pointer;
  background-color:#990000;
  position:relative;
  top:10px;
  left:35%;
  margin-left:auto;
  margin-right:auto;
}

.reboot{
  clear:both;
  font-weight: 300;
  font-size: 16px;
  text-decoration: none;
  text-align: center;
  line-height: 40px;
  height: 10%;
  padding: 0 3%;
  margin: 0;
  display: inline-block;
  appearance: none;
  cursor: pointer;
  background-color:#66FF66;
  position:relative;
  top:10px;
  left:40%;
  margin-left:auto;
  margin-right:auto;
}

.auto_boot_and_shutdown_checkbox{
  position:relative;
  left:60%;
  top:20px;
  width:20%;
}

.auto_boot_and_shutdown_label{
  color: #999999;
  font-size:150%;
  position:relative;
  left:40%;
  top:45px;
  width:30%;
}
.auto_boot_label{
  color: #999999;
  font-size:150%;
  position:relative;
  left:25%;
  top:60px;
  width:30%;
}
.boot_time{
  position:relative;
  left:50%;
  top:35px;
}

.auto_shutdown_label{
  color: #999999;
  font-size:150%;
  position:relative;
  left:25%;
  top:45px;
  width:30%;
}

.shutdown_time{
  position:relative;
  left:50%;
  top:20px;
}
.set_ok{
  clear:both;
  font-weight: 300;
  font-size: 16px;
  text-decoration: none;
  text-align: center;
  line-height: 40px;
  height: 10%;
  padding: 0 3%;
  margin: 0;
  display: inline-block;
  appearance: none;
  cursor: pointer;
  background-color:#D0D0D0;
  position:relative;
  top:60px;
  left:30%;
  margin-left:auto;
  margin-right:auto;
}

.cancel_boot{
  position:relative;
  top:30px;
  left:50%;
}
.cancel_shutdown{
  position:relative;
  top:20px;
  left:50%;
}
